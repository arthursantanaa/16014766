public class Qst10 {
	public static Scanner s = new Scanner (System.in);
    public static void main (String[] args ) {

    	System.out.println("Informe sua altura: ");
    	
    	double A = s.nextDouble();
    	
    	double P = (72.7*A)-58;
    	
    	System.out.println("Seu peso ideal Ã© : "+P+"Kg");
    	
    	
    	
    	
    }	
}
